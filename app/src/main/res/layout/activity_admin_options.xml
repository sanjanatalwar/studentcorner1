        android:textSize="30dp"
        android:textStyle="italic"
        android:textColor="@color/colorWhite"
        android:background="@drawable/button__shape"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.103"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteX="32dp" />

    <Button
        android:id="@+id/adminbca2list"
        android:layout_width="306dp"
        android:layout_height="94dp"
        android:text="List Of Bca2"
        android:textSize="30dp"
        android:onClick="bca2Click"
        android:textStyle="italic"
        android:textColor="@color/colorWhite"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:background="@drawable/button__shape"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="0.394" />

    <Button
        android:id="@+id/adminbca3list"
        android:layout_width="310dp"
        android:layout_height="84dp"
        android:text="List of Bca3"
        android:textSize="30dp"
        android:onClick="bcaThreeclick"
        android:textStyle="italic"
        android:textColor="@color/colorWhite"
        android:background="@drawable/button__shape"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.679"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteX="14dp" />
    <Button
        android:id="@+id/adminTeacherlist"
        android:layout_width="310dp"
        android:layout_height="84dp"
        android:text="List of Teachers"
        android:textSize="30dp"
        android:onClick="teacherclick"
        android:textStyle="italic"
        android:textColor="@color/colorWhite"
        android:background="@drawable/button__shape"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.948"
        app:layout_constraintHorizontal_bias="1.0" />



</android.support.constraint.ConstraintLayout>
